while True:
	a=(raw_input('Nhap cac chuoi de hien thi sticker - Nhan Q de thoat'))
	if a =='q':
		print 'Ban vua nhap phim Q! Chuong trinh se thoat'
		break
	else:
		try:
			if a=='1x':
				print (':)')
			elif a=='2x':
				print (':))')
			elif a=='3x':
			print ('=))')
			elif a=='4x':
				print ('<3') 
		except:
			print 'Khong co ky tu nao duoc hien thi'
		

	
